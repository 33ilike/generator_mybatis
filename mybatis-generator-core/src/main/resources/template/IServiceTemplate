     *
     * @param ${modelName}
     */
    Result create${modelClazz}(${modelClazz} ${modelName});

    /**
     * 删除
     *
     * @param ids id
     * @return
     */
    Result delete${modelClazz}(String ids);

    /**
     * 修改
     *
     * @param ${modelName} id
     * @return
     */
    Result update${modelClazz}(${modelClazz} ${modelName});

    /**
     * 查询
     *
     * @param parameterMap 参数
